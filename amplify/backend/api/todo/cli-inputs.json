{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "todo",
    "serviceName": "AppSync",
    "gqlSchemaPath": "/home/dtal/repos/todo/amplify/backend/api/todo/schema.graphql",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "This is an api key",
      "expirationTime": 30
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Todo"
        }
      ]
    }
  }
}